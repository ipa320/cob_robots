<?xml version="1.0"?>
<launch>

	<arg name="robot_env" default="$(optenv ROBOT_ENV !!NO_ROBOT_ENV_SET!!)"/>
	<arg name="pkg_env_config" default="$(find cob_default_env_config)"/>

	<include file="$(find cob_bringup)/robots/cob3-5b.xml" >
		<arg name="pc1" value="cob3-5-pc1"/>
		<arg name="pc2" value="cob3-5-pc2"/>
		<arg name="pc3" value="cob3-5-pc3"/>
	</include>
	
	<!-- upload default configuration parameters -->
	<include file="$(arg pkg_env_config)/upload_navigation_goals.launch">
		<arg name="robot_env" value="$(arg robot_env)" />
	</include>

</launch>
