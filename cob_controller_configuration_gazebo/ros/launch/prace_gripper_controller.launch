<?xml version="1.0"?>
<launch>

        <arg name="robot" />

        <rosparam file="$(find prace_gripper_gazebo)/controller/prace_gripper_controller.yaml" command="load"/>

        <node name="gripper_controller_spawner" pkg="pr2_controller_manager" type="spawner" args="gripper_controller" />

        <group ns="gripper_controller">
                <node name="gripper_joint_trajectory_action_node" pkg="joint_trajectory_action" type="joint_trajectory_action" />
        </group>

</launch>

