<?xml version="1.0"?>
<launch>

	<!-- send ROBOT parameters to parameter server -->
	<rosparam command="load" ns="robot_modules" file="$(find cob_default_robot_config)/desire/robot_modules.yaml"/>
	<rosparam command="load" ns="head" file="$(find cob_default_robot_config)/desire/head_joint_configurations.yaml"/>
	<rosparam command="load" ns="arm_right" file="$(find cob_default_robot_config)/desire/arm_right_joint_configurations.yaml"/>
	<rosparam command="load" ns="arm_left" file="$(find cob_default_robot_config)/desire/arm_left_joint_configurations.yaml"/>
	<rosparam command="load" ns="sdh_right" file="$(find cob_default_robot_config)/desire/sdh_right_joint_configurations.yaml"/>
	<rosparam command="load" ns="sdh_left" file="$(find cob_default_robot_config)/desire/sdh_left_joint_configurations.yaml"/>
	<rosparam command="load" ns="sound" file="$(find cob_default_robot_config)/desire/sound.yaml"/>

</launch>
