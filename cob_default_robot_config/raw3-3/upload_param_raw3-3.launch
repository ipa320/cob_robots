<?xml version="1.0"?>
<launch>

	<!-- send ROBOT parameters to parameter server -->
	<rosparam command="load" ns="/script_server/arm_left" file="$(find cob_default_robot_config)/raw3-3/arm_left_joint_configurations.yaml"/>
	<rosparam command="load" ns="/script_server/arm_right" file="$(find cob_default_robot_config)/raw3-3/arm_right_joint_configurations.yaml"/>
	<rosparam command="load" ns="/script_server/torso" file="$(find cob_default_robot_config)/raw3-3/torso_joint_configurations.yaml"/>
	<rosparam command="load" ns="/script_server/lookat" file="$(find cob_default_robot_config)/raw3-3/lookat_joint_configurations.yaml"/>
	<rosparam command="load" ns="/script_server/gripper_left" file="$(find cob_default_robot_config)/raw3-3/gripper_left_joint_configurations.yaml"/>
	<rosparam command="load" ns="/script_server/gripper_right" file="$(find cob_default_robot_config)/raw3-3/gripper_right_joint_configurations.yaml"/>
	<rosparam command="load" ns="/script_server/light" file="$(find cob_default_robot_config)/raw3-1/light_configurations.yaml"/>

</launch>
