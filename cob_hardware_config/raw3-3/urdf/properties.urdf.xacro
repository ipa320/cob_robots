	<xacro:property name="neck_yaw" value="${def_neck_yaw+offset_neck_yaw}"/>

	<!-- head mount positions | relative to neck_mount_link -->
	<xacro:property name="head_x" value="${def_head_x+offset_head_x}"/>
	<xacro:property name="head_y" value="${def_head_y+offset_head_y}"/>
	<xacro:property name="head_z" value="${def_head_z+offset_head_z}"/>
	<xacro:property name="head_roll" value="${def_head_roll+offset_head_roll}"/>
	<xacro:property name="head_pitch" value="${def_head_pitch+offset_head_pitch}"/>
	<xacro:property name="head_yaw" value="${def_head_yaw+offset_head_yaw}"/>

	<!-- ids_middle mount positions | relative to head_cam_holder_link -->
	<xacro:property name="ids_middle_x" value="${def_ids_middle_x+offset_ids_middle_x}"/>
	<xacro:property name="ids_middle_y" value="${def_ids_middle_y+offset_ids_middle_y}"/>
	<xacro:property name="ids_middle_z" value="${def_ids_middle_z+offset_ids_middle_z}"/>
	<xacro:property name="ids_middle_roll" value="${def_ids_middle_roll+offset_ids_middle_roll}"/>
	<xacro:property name="ids_middle_pitch" value="${def_ids_middle_pitch+offset_ids_middle_pitch}"/>
	<xacro:property name="ids_middle_yaw" value="${def_ids_middle_yaw+offset_ids_middle_yaw}"/>

	<!-- ids_left mount positions | relative to head_cam_holder_link -->
	<xacro:property name="ids_left_x" value="${def_ids_left_x+offset_ids_left_x}"/>
	<xacro:property name="ids_left_y" value="${def_ids_left_y+offset_ids_left_y}"/>
	<xacro:property name="ids_left_z" value="${def_ids_left_z+offset_ids_left_z}"/>
