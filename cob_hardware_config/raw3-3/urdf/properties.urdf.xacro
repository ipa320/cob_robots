	<xacro:property name="ids_left_yaw" value="${def_ids_left_yaw+offset_ids_left_yaw}"/>

	<!-- ids_right mount positions | relative to head_cam_holder_link -->
	<xacro:property name="ids_right_x" value="${def_ids_right_x+offset_ids_right_x}"/>
	<xacro:property name="ids_right_y" value="${def_ids_right_y+offset_ids_right_y}"/>
	<xacro:property name="ids_right_z" value="${def_ids_right_z+offset_ids_right_z}"/>
	<xacro:property name="ids_right_roll" value="${def_ids_right_roll+offset_ids_right_roll}"/>
	<xacro:property name="ids_right_pitch" value="${def_ids_right_pitch+offset_ids_right_pitch}"/>
	<xacro:property name="ids_right_yaw" value="${def_ids_right_yaw+offset_ids_right_yaw}"/>

	<!-- cam_holder mount positions | relative to head_powerball_knob_link -->
	<xacro:property name="cam_holder_x" value="${def_cam_holder_x+offset_cam_holder_x}"/>
	<xacro:property name="cam_holder_y" value="${def_cam_holder_y+offset_cam_holder_y}"/>
	<xacro:property name="cam_holder_z" value="${def_cam_holder_z+offset_cam_holder_z}"/>
	<xacro:property name="cam_holder_roll" value="${def_cam_holder_roll+offset_cam_holder_roll}"/>
	<xacro:property name="cam_holder_pitch" value="${def_cam_holder_pitch+offset_cam_holder_pitch}"/>
	<xacro:property name="cam_holder_yaw" value="${def_cam_holder_yaw+offset_cam_holder_yaw}"/>

</robot>
