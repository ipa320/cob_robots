<?xml version="1.0"?>
<launch>

    <!-- upload parameter -->
    <include file="$(find cob_default_robot_config)/upload_param.launch" />
    <!-- perform test -->
    <test test-name="test_calibration" pkg="cob_hardware_test" type="calibration_test.py" name="test_calibration" >
     <param name="test_target_arm"  value="pregrasp" />
     <param name="test_target_torso" value="back" />
     <param name="test_target_head" value="back" />
     <param name="wait_time" value="10.0" />
    </test>

</launch>
