<?xml version="1.0"?>
<launch>

    <!-- upload parameter -->
    <include file="$(find cob_default_robot_config)/upload_param.launch" />    
    <!-- perform test -->
    <test test-name="test_arm" pkg="cob_hardware_test" type="component_test.py" name="cob_arm" >
     <param name="component"  value="arm" />
     <param name="test_target" value="pregrasp" />
     <param name="default_target" value="folded" />
     <param name="wait_time" value="30.0" />
     <param name="error_range" value="0.1" />
    </test>

</launch>
