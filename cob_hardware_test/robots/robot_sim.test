<?xml version="1.0"?>
<launch>

	<!-- start dialog service -->
	<node pkg="cob_hardware_test" type="dialog.py" name="dialog_server"/>
	<!-- bringup robot -->
	<include file="$(find cob_bringup_sim)/launch/robot.launch" />
	<!-- include auto test -->
	<include file="$(find cob_hardware_test)/robots/$(env ROBOT)_auto.test.xml" />
	<!-- include manual test -->
	<include file="$(find cob_hardware_test)/robots/$(env ROBOT)_manual.test.xml" />

</launch>
